{
  "name": "triggerskit",
  "description": "One type-safe SDK for all platforms, Telegram, Slack, Discord & more",
  "version": "0.1.15",
  "type": "module",
  "files": [
    "dist"
  ],
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./storage": {
      "import": {
        "types": "./dist/storage/index.d.ts",
        "default": "./dist/storage/index.js"
      }
    },
    "./frameworks/hono": {
      "import": {
        "types": "./dist/frameworks/hono.d.ts",
        "default": "./dist/frameworks/hono.js"
      }
    },
    "./package.json": "./package.json"
  },
  "dependencies": {
    "@triggerskit/core": "workspace:*"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zydeer/triggerskit.git"
  },
  "homepage": "https://github.com/zydeer/triggerskit/tree/main/packages/triggerskit#readme",
  "peerDependencies": {
    "hono": ">=4.0.0"
  }
}
