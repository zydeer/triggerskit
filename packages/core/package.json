{
  "name": "@triggerskit/core",
  "description": "Core utilities for TriggersKit providers",
  "version": "0.1.15",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    "./oauth": {
      "import": {
        "types": "./dist/oauth.d.ts",
        "default": "./dist/oauth.js"
      }
    },
    "./http": {
      "import": {
        "types": "./dist/http.d.ts",
        "default": "./dist/http.js"
      }
